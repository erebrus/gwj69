SceneManager="*res://addons/scene_manager/SceneManager.tscn"
Types="*res://src/common/types.gd"
Events="*res://src/common/events.gd"
HyperLog="*res://addons/hyperlog/hyperlog.tscn"
Globals="*res://src/common/globals.tscn"

[display]

window/size/viewport_width=1280
window/size/viewport_height=760
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=0

[editor_plugins]

